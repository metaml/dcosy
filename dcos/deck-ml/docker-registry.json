{
  "id": "docker-registry",
  "cpus": 1.0,
  "mem": 1024,
  "instances": 1,
  "container": {
    "docker": {"image": "registry"},
    "forcePullImage": true,
    "type": "DOCKER"
  },
  "env": {"SETTINGS_FLAVOR": "local", "STORAGE_PATH": "/docker_storage"},
  "constraints": [["hostname", "UNIQUE"]]
}
